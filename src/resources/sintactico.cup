terminal String IGUAL,ABRIR_PARENTESIS,CERRAR_PARENTESIS,ABRIR_LLAVE,CERRAR_LLAVE, CORCHETE_ABRE, CORCHETE_CIERRA,COMA, SUMA, RESTA, DIVISION,MULTIPLICACION,AND,OR,NOT;
