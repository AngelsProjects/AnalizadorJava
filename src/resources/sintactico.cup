non terminal EstructuraMientras, HacerMientras, Paqueteria, ElInterrumputor;
non terminal EstructuraClase,Programa,ClasePrincipal, CentroCondicional;
Start with Programa;
Programa ::= Paqueteria EstructuraClase|Paqueteria Importacion EstructuraClase|error;

Paqueteria ::= PAQUETE VARIABLE PaqueteriaRuta PUNTOYCCOMA | PAQUETE VARIABLE PUNTOYCOMA | error ;
PaqueteriaRuta ::= PUNTO VARIABLE | PUNTO VARIABLE PaqueteriaRuta | error;

Importacion ::= IMPORTAR VARIABLE PUNTOYCOMA| IMPORTAR VARIABLE Importacion2 PUNTOYCOMA;
Importacion2 ::= PUNTO VARIABLE | PUNTO VARIABLE Importancion2 | error ;

ClasePrincipal ::= PUBLICO ESTATICO HUECO PRINCIPAL ABRIR_PARENTESIS STRING 
                    CORCHETE_ABRE CORCHETE_CIERRA ARGUMENTO CERRAR_PARENTESIS 
                    ABRIR_LLAVE EstructuraClase CERRAR_LLAVE | error;

EstructuraClase ::= DarvalorVariable | DarvalorVariable EstructuraClase |
                    Condicionif | Condicionif EstructuraClase |
                    EstructuraMientras | EstructuraMientras EstructuraClase |
                    HacerMientras | HacerMientras EstructuraClase |
                    error;

DarvalorVariable ::= PRIVADO DarvalorVariable2 | PUBLICO DarvalorVariable2 | PROTEGIDO DarvalorVariable2 | ESTATICO DarvalorVariable2 | DarvalorVariable2 | error;

DarvalorVariable2 ::= ENTERO VARIABLE PUNTOYCOMA | ENTERO VARIABLE IGUAL VALORINT PUNTOYCOMA |
                    FLOTANTE VARIABLE PUNTOYCOMA | FLOTANTE VARIABLE IGUAL VALORFLOAT PUNTOYCOMA |
                    DOBLE VARIABLE PUNTOYCOMA | DOBLE VARIABLE IGUAL VALORDOBLE PUNTOYCOMA |
                    BOOLEANO VARIABLE PUNTOYCOMA | BOOLEANO VARIABLE IGUAL VALORBOOLEANO PUNTOYCOMA |
                    STRING VARIABLE PUNTOYCOMA | STRING VARIABLE IGUAL COMILLA CADENA COMILLA PUNTOYCOMA | error;
                        
Condicionif ::= SI ABRIR_PARENTESIS CentroCondicional CERRAR_PARENTESIS ABRIR_LLAVE EstructuraClase CERRAR_LLAVE |
                    SI ABRIR_PARENTESIS CentroCondicional CERRAR_PARENTESIS ABRIR_LLAVE EstructuraClase CERRAR_LLAVE SINO ABRIR_LLAVE EstructuraClase CERRAR_LLAVE | error;

EstructuraMientras ::= MIENTRAS ABRIR_PARENTESIS CentroCondicional CERRAR_PARENTESIS ABRIR_LLAVE EstructuraClase CERRAR_LLAVE | error;

HacerMientras ::= HACER ABRIR_LLAVE EstructuraClase CERRAR_LLAVE MIENTRAS ABRIR_PARENTESIS CentroCondicional CERRAR_PARENTESIS ABRIR_LLAVE EstructuraClase CERRAR_LLAVE | error;

